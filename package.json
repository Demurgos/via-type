{
  "name": "kryo",
  "version": "0.5.0-alpha.11",
  "description": "Serialization for documents.",
  "main": "dist/lib/index.js",
  "types": "dist/lib/index.d.ts",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "scripts": {
    "prepare": "gulp all:tsconfig.json && gulp :tslint.json && gulp all:dist",
    "test": "gulp :lint && gulp test",
    "prestart": "gulp main:build",
    "start": "npm test",
    "prepublishOnly": "npm test",
    ":lint": "gulp :lint",
    "typedoc": "gulp lib:typedoc",
    "gh-pages": "rm -rf typedoc/ && gulp lib:typedoc && touch typedoc/.nojekyll && cp -r typedoc/. dist/gh-pages/"
  },
  "pre-commit": {
    "run": [
      "test"
    ]
  },
  "keywords": [],
  "license": "MIT",
  "dependencies": {
    "@types/bson": "^1.0.3",
    "@types/unorm": "^1.3.27",
    "incident": "^2.0.0"
  },
  "optionalDependencies": {
    "bson": "^1.0.4",
    "unorm": "^1.4.1"
  },
  "devDependencies": {
    "@types/chai": "^3.5.2",
    "@types/mocha": "^2.2.41",
    "@types/node": "^8.0.20",
    "@types/qs": "^6.2.31",
    "chai": "^3.5.0",
    "demurgos-web-build-tools": "^0.15.0-beta.5",
    "gulp": "github:gulpjs/gulp#4.0",
    "gulp-cli": "^1.4.0",
    "pre-commit": "^1.2.2",
    "qs": "^6.5.0",
    "ts-node": "^3.3.0",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2"
  }
}

{
  "name": "kryo-testing",
  "version": "0.9.0",
  "description": "Helpers to test Kryo types and serializers",
  "license": "MIT",
  "keywords": [],
  "homepage": "https://demurgos.github.io/kryo",
  "repository": {
    "type": "git",
    "url": "git://github.com/demurgos/kryo.git"
  },
  "private": false,
  "type": "module",
  "files": [
    "./lib/"
  ],
  "main": "./lib/index.js",
  "exports": {
    ".": "./lib/index.js",
    "./lib/": "./lib/"
  },
  "engines": {
    "node": ">=13.7"
  },
  "scripts": {
    "build": "tsc --build src/lib/tsconfig.json",
    "pretest": "tsc --build src/test/tsconfig.json",
    "test": "c88 --reporter html --reporter text --reporter lcov-file mocha \"test/**/*.spec.js\"",
    "posttest": "yarn run lint",
    "watch": "gulp lib:watch",
    "lint": "eslint \"./src/**/*.ts\"",
    "format": "eslint --fix \"./src/**/*.ts\""
  },
  "dependencies": {
    "kryo": "^0.9.0",
    "tslib": "^1.11.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.8",
    "@types/mocha": "^7.0.1",
    "c88": "^0.3.4",
    "chai": "^4.2.0",
    "mocha": "^7.1.1",
    "typescript": "^3.7.5"
  },
  "c88": {
    "match": [
      "./lib/**/*.js"
    ]
  }
}
